# Database Connection Configuration Sample
# 이 파일을 복사하여 db.properties로 이름을 변경하고 실제 값을 입력하세요.
# cp db.properties.sample db.properties

db.url=jdbc:vertica://YOUR_DB_HOST:5433/YOUR_DB_NAME
db.user=YOUR_DB_USER
db.password=YOUR_DB_PASSWORD
db.driver=com.vertica.jdbc.Driver

# HikariCP Pool Settings
hikari.maximumPoolSize=20
hikari.minimumIdle=5
hikari.connectionTimeout=30000
hikari.idleTimeout=600000
hikari.maxLifetime=1800000
